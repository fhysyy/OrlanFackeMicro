# FakeMicro 前端环境变量示例
# 复制此文件为 .env 并修改相应配置

# API基础地址
VITE_API_BASE_URL=http://localhost:5000

# 应用标题
VITE_APP_TITLE=FakeMicro Management

# 应用描述
VITE_APP_DESCRIPTION=基于Orleans的微服务管理平台

# 是否启用调试模式
VITE_DEBUG=true

# 是否启用PWA
VITE_ENABLE_PWA=false

# 是否启用分析工具
VITE_ENABLE_ANALYTICS=false

# 分析工具ID（如Google Analytics）
VITE_ANALYTICS_ID=

# 是否启用暗色主题
VITE_ENABLE_DARK_MODE=true

# 默认语言
VITE_DEFAULT_LANGUAGE=zh-CN

# 请求超时时间（毫秒）
VITE_REQUEST_TIMEOUT=10000

# 上传文件大小限制（MB）
VITE_UPLOAD_FILE_SIZE_LIMIT=10

# 允许上传的文件类型
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# 分页默认大小
VITE_DEFAULT_PAGE_SIZE=20

# 是否启用实时通知
VITE_ENABLE_REALTIME_NOTIFICATIONS=true

# WebSocket地址
VITE_WS_URL=ws://localhost:5000/ws

# 地图服务密钥（如需要地图功能）
VITE_MAP_API_KEY=

# 第三方登录配置
VITE_ENABLE_OAUTH=false
VITE_GOOGLE_CLIENT_ID=
VITE_GITHUB_CLIENT_ID=
VITE_WECHAT_APP_ID=

# 缓存配置
VITE_CACHE_ENABLED=true
VITE_CACHE_EXPIRE_TIME=3600000

# 性能监控配置
VITE_ENABLE_PERFORMANCE_MONITOR=true
VITE_PERFORMANCE_THRESHOLD=1000