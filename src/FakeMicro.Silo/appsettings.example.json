{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Orleans": "Information",
      "FakeMicro.DatabaseAccess.Services.DatabaseInitializerHostedService": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=fakemicro;Username=postgres;Password=your_password"
  },
  "SqlSugar": {
    "DbType": "PostgreSQL",
    "ConnectionString": "Host=localhost;Port=5432;Database=fakemicro;Username=postgres;Password=your_password",
    "EnableSqlLog": true,
    "SlowQueryThreshold": 2000,
    "EnableAop": true,
    "ConnectionPoolSize": 50,
    "ConnectionTimeout": 30,
    "EnableReadWriteSeparation": false,
    "SlaveConnectionStrings": []
  },
  "DatabaseInitializer": {
    "EnableSeedData": true,
    "EnableIndexCreation": true,
    "RecreateTablesInDevelopment": false,
    "MigrationVersion": "1.0.0"
  },
  "Orleans": {
    "ClusterId": "FakeMicroCluster",
    "ServiceId": "FakeMicroService",
    "AdvertisedIP": "127.0.0.1",
    "SiloPort": 11111,
    "GatewayPort": 30000,
    "DashboardPort": 8080,
    "EnableDashboard": true
  },
  "Jwt": {
    "SecretKey": "your-super-secret-jwt-key-that-is-at-least-256-bits-long",
    "Issuer": "FakeMicro",
    "Audience": "FakeMicro.Users",
    "ExpirationMinutes": 1440
  }
}